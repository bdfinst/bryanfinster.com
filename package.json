{
  "name": "bryanfinster.com",
  "version": "1.0.0",
  "description": "bryanfinster.com",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bdfinst/bryanfinster.com"
  },
  "author": "Bryan Finster",
  "license": "MIT",
  "scripts": {
    "develop": "gatsby develop",
    "start": "npm run clean && npm run develop",
    "build": "npm run clean && gatsby build",
    "deploy": "npm run clean && gatsby build --prefix-paths && gh-pages -d public",
    "clean": "rimraf .cache public",
    "serve": "gatsby serve",
    "now-build": "npm run build",
    "format": "npm run format:fix && npm run lint:js",
    "format:fix": "prettier --write '**/*.{js,jsx,yml,json,css}'",
    "lint:js": "eslint --cache --ext .js,.jsx . --fix",
    "lint": "concurrently 'npm run lint:js' 'npm run format:fix'",
    "medium": "node node_modules/medium-2-md convertLocal ~/Downloads/posts -i -f"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.9.4",
    "gatsby": "^2.27.4",
    "gatsby-plugin-manifest": "^2.7.0",
    "gatsby-plugin-offline": "^3.5.0",
    "gatsby-plugin-react-helmet": "^3.5.0",
    "gatsby-plugin-styled-components": "^3.5.0",
    "gatsby-plugin-typography": "^2.7.0",
    "gatsby-remark-prismjs": "^3.8.0",
    "gatsby-source-filesystem": "^2.6.1",
    "gatsby-source-rss-feed": "^1.2.2",
    "gatsby-transformer-remark": "^2.11.1",
    "gatsby-transformer-yaml": "^2.6.0",
    "prismjs": "^1.15.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "react-icons": "^3.2.2",
    "react-typography": "^0.16.18",
    "styled-components": "^4.1.2",
    "typescript": "^4.1.2",
    "typography": "^0.16.18",
    "typography-theme-bootstrap": "^0.16.18"
  },
  "devDependencies": {
    "cheerio": "^1.0.0-rc.3",
    "concurrently": "5.3.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jest": "23.8.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "7.19.0",
    "eslint-watch": "6.0.1",
    "js-yaml": "^3.14.0",
    "medium-2-md": "^0.5.1",
    "node-fetch": "^2.6.1",
    "rimraf": "^3.0.2",
    "turndown": "^7.0.0"
  }
}
