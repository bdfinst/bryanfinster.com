{
  "name": "bryanfinster.com",
  "version": "1.0.0",
  "description": "bryanfinster.com",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bdfinst/bryanfinster.com"
  },
  "author": "Bryan Finster",
  "license": "MIT",
  "scripts": {
    "develop": "gatsby develop",
    "start": "npm run clean && npm run develop",
    "build": "npm run clean && gatsby build",
    "deploy": "npm run clean && gatsby build --prefix-paths && gh-pages -d public",
    "clean": "rimraf .cache public",
    "serve": "gatsby serve",
    "now-build": "npm run build",
    "format": "npm run format:fix && npm run lint:js",
    "format:fix": "prettier --write '**/*.{js,jsx,yml,json,css}'",
    "lint:js": "eslint --cache --ext .js,.jsx . --fix",
    "lint": "concurrently 'npm run lint:js' 'npm run format:fix'",
    "medium": "node node_modules/medium-2-md convertLocal ~/Downloads/posts -i -f"
  },
  "importSort": {
    ".js, .jsx": {
      "parser": "babylon",
      "style": "absolute",
      "options": {}
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "babel-plugin-styled-components": "^1.9.4",
    "date-fns": "^2.16.1",
    "gatsby": "^2.32.4",
    "gatsby-plugin-manifest": "^2.12.0",
    "gatsby-plugin-material-ui": "^2.1.10",
    "gatsby-plugin-offline": "^3.10.1",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-plugin-styled-components": "^3.10.0",
    "gatsby-plugin-typography": "^2.12.0",
    "gatsby-remark-prismjs": "^3.13.0",
    "gatsby-source-filesystem": "^2.11.0",
    "gatsby-source-rss-feed": "^1.2.2",
    "gatsby-transformer-remark": "^2.16.0",
    "gatsby-transformer-yaml": "^2.11.0",
    "html2canvas": "^1.0.0-rc.7",
    "jspdf": "^2.1.1",
    "material-ui-image": "^3.3.0",
    "prismjs": "^1.15.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "react-icons": "^3.2.2",
    "react-typography": "^0.16.18",
    "semantic-ui-react": "^2.0.1",
    "styled-components": "^4.1.2",
    "typescript": "^4.1.5",
    "typography": "^0.16.18",
    "typography-theme-bootstrap": "^0.16.18"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "cheerio": "^1.0.0-rc.5",
    "concurrently": "^6.0.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-watch": "^7.0.0",
    "husky": "^5.1.3",
    "import-sort-style-absolute": "^1.0.1",
    "js-yaml": "^3.14.0",
    "medium-2-md": "^0.5.1",
    "node-fetch": "^2.6.1",
    "rimraf": "^3.0.2",
    "turndown": "^7.0.0"
  }
}
