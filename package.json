  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "npm run medium && gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "clean": "rm -rf public && rm -rf .cache",
    "develop": "gatsby develop",
    "format": "npm run format:fix && npm run lint:js",
    "format:check": "prettier --list-different '**/*.{js,jsx,yml,json,css}'",
    "format:fix": "prettier --write '**/*.{js,jsx,yml,json,css}'",
    "lint:js": "eslint --ext .js,.jsx . --fix",
    "lint:md": "remark content/posts/",
    "write-good": "write-good $(glob 'content/posts/**/*.md')",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "jest",
    "medium": "node medium.js",
    "medium2": "node src/medium-2-markdown/index.js"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
